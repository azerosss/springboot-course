<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ch09springbootmapper.dao.StudentMapper">

    <!--定义sql语句-->
    <select id="selectById" resultType="com.example.ch09springbootmapper.domain.Student">
        select id,name,age from student where id=#{stuId}
    </select>

</mapper>